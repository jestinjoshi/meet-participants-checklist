{
    "name": "Google Meet Participants Checklist",
    "version": "0.0.5",
    "description": "Adds a checkbox next to each Google Meet participant",
    "manifest_version": 3,
    "icons": {
        "16": "images/gmeet_16.png",
        "32": "images/gmeet_32.png",
        "48": "images/gmeet_48.png",
        "128": "images/gmeet_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "images/selected.svg"
            ],
            "matches": [
                "https://meet.google.com/*"
            ]
        }
    ]
}